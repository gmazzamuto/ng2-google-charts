{
  "name": "ng2-google-charts",
  "version": "3.3.1",
  "description": "Angular2 Google Charts module",
  "private": true,
  "scripts": {
    "demo.gh-pages": "run-s build demo.build demo.deploy",
    "demo.ng-build": "ng build -prod --aot --preserve-symlinks -nc",
    "demo.deploy": "gh-pages -d demo/dist",
    "demo.build": "run-s build link demo.ng-build",
    "link": "ngm link -p src --here",
    "lint": "tslint src/**/*.ts src/*.ts --exclude src/**/*.spec.ts src/*.spec.ts",
    "disable-lint": "tslint \"**/*.ts\" -c tslint.json --fix --type-check -t prose -e \"node_modules/**\" -e \"dist/**\" -e \"temp/**\" -e \"scripts/docs/**\"",
    "build": "run-s lint build.ngm link",
    "build.ngm": "ngm build -p src --clean",
    "build.watch": "ngm build -p src --watch --skip-bundles",
    "start": "ng serve --aot"
  },
  "main": "bundles/ng2-google-charts.umd.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "keywords": [
    "angular",
    "angular2",
    "ng",
    "ng2",
    "Google",
    "Charts"
  ],
  "author": "Jago MF",
  "originalAuthor": "Giacomo Mazzamuto",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jagomf/ng2-google-charts.git"
  },
  "bugs": {
    "url": "https://github.com/jagomf/ng2-google-charts/issues"
  },
  "homepage": "https://github.com/jagomf/ng2-google-charts",
  "dependencies": {
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "1.6.7",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/core": "^5.2.3",
    "@types/marked": "0.3.0",
    "@types/node": "~6.0.60",
    "@types/webpack": "3.0.10",
    "bootstrap": "4.0.0",
    "classlist-polyfill": "1.2.0",
    "codelyzer": "3.2.0",
    "core-js": "2.5.1",
    "cpy": "5.1.0",
    "cpy-cli": "1.0.1",
    "gh-pages": "1.0.0",
    "gitignore-to-glob": "0.3.0",
    "ngm-cli": "0.8.2",
    "npm-run-all": "4.1.1",
    "reflect-metadata": "0.1.10",
    "require-dir": "0.3.2",
    "rxjs": "^5.5.6",
    "ts-helpers": "^1.1.1",
    "tslint-config-valorsoft": "2.1.1",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3",
    "webdriver-manager": "12.0.6",
    "zone.js": "^0.8.19"
  }
}
